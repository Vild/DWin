import std.string;

private XCBKeyParser keyParser;
private immutable static Key[string] keys;
private alias ParseKey = keyParser.ParseKey;

class XCBKeyParser : IKeyParser {
public:
	void Refresh(XCBEngine engine) {
		refreshNumlockMask(engine);
		refreshModifiers(engine);
	}

	override Key ParseKey(string key) {
		key = key.toLower;
		if (auto val = key in keys)
			return *val;
		else
			return keys["None".toLower];
	}

	override Modifier ParseModifier(string mod) {
		mod = mod.toLower;
		if (auto val = mod in modifiers)
			return *val;
		else
			return Modifier.None;
	}

	override MouseButton ParseMouseButton(string button) {
		button = button.toLower;
		if (button.length > "button".length && button[0 .. "button".length] == "button") {
			int idx = button["button".length] - '1';
			if (idx >= 0 && idx <= 5)
				return cast(MouseButton)(MouseButton.Button1 + idx);
		}

		return MouseButton.None;
	}

	@property override uint NumlockMask() {
		return numlockMask;
	}

	@property override uint MouseMasks() {
		return mouseMasks;
	}

private:
	Modifier[string] modifiers;
	uint numlockMask;
	uint mouseMasks = XCB_KEY_BUT_MASK_BUTTON_1 + XCB_KEY_BUT_MASK_BUTTON_2 + XCB_KEY_BUT_MASK_BUTTON_3 + XCB_KEY_BUT_MASK_BUTTON_4 + XCB_KEY_BUT_MASK_BUTTON_5;
	void refreshNumlockMask(XCBEngine engine) {
		numlockMask = 0; //; // Removes mouse buttons flags
		xcb_get_modifier_mapping_reply_t* reply = xcb_get_modifier_mapping_reply(engine.Connection,
			xcb_get_modifier_mapping(engine.Connection), null);
		if (!reply)
			return;
		scope (exit)
			xcb_free(reply);

		xcb_keycode_t* codes = xcb_get_modifier_mapping_keycodes(reply);
		enum XK_Num_Lock = 0xff7f; //TODO: FIX THIS
		xcb_keycode_t* target = xcb_key_symbols_get_keycode(engine.Symbols, XK_Num_Lock);

		if (!target)
			return;

		for (uint i = 0; i < 8; i++)
			for (uint j = 0; j < reply.keycodes_per_modifier; j++)
				if (codes[i * reply.keycodes_per_modifier + j] == *target)
					numlockMask |= (1 << i);
	}

	void refreshModifiers(XCBEngine engine) {
		modifiers = null;
		//dfmt off
		xcb_keycode_t*[string] mods = [
			"Shift_L".toLower : xcb_key_symbols_get_keycode(engine.Symbols, "Shift_L".ParseKey),
			"Shift_R".toLower : xcb_key_symbols_get_keycode(engine.Symbols, "Shift_R".ParseKey),
			"Control_L".toLower : xcb_key_symbols_get_keycode(engine.Symbols, "Control_L".ParseKey),
			"Control_R".toLower : xcb_key_symbols_get_keycode(engine.Symbols, "Control_R".ParseKey),
			"Meta_L".toLower : xcb_key_symbols_get_keycode(engine.Symbols, "Meta_L".ParseKey),
			"Meta_R".toLower : xcb_key_symbols_get_keycode(engine.Symbols, "Meta_R".ParseKey),
			"Alt_L".toLower : xcb_key_symbols_get_keycode(engine.Symbols, "Alt_L".ParseKey),
			"Alt_R".toLower : xcb_key_symbols_get_keycode(engine.Symbols, "Alt_R".ParseKey),
			"Super_L".toLower : xcb_key_symbols_get_keycode(engine.Symbols, "Super_L".ParseKey),
			"Super_R".toLower : xcb_key_symbols_get_keycode(engine.Symbols, "Super_R".ParseKey),
			"Hyper_L".toLower : xcb_key_symbols_get_keycode(engine.Symbols, "Hyper_L".ParseKey),
			"Hyper_R".toLower : xcb_key_symbols_get_keycode(engine.Symbols, "Hyper_R".ParseKey),

			"Caps_Lock".toLower : xcb_key_symbols_get_keycode(engine.Symbols, "Caps_Lock".ParseKey),
			"Shift_Lock".toLower : xcb_key_symbols_get_keycode(engine.Symbols, "Shift_Lock".ParseKey),
			"Num_Lock".toLower : xcb_key_symbols_get_keycode(engine.Symbols, "Num_Lock".ParseKey),
			"Scroll_Lock".toLower : xcb_key_symbols_get_keycode(engine.Symbols, "Scroll_Lock".ParseKey),

			"Mode_switch".toLower : xcb_key_symbols_get_keycode(engine.Symbols, "Mode_switch".ParseKey)
		];
		//dfmt on

		xcb_get_modifier_mapping_reply_t* modmapReply = xcb_get_modifier_mapping_reply(engine.Connection,
			xcb_get_modifier_mapping(engine.Connection), null);
		if (!modmapReply)
			return;

		xcb_keycode_t* modmap = xcb_get_modifier_mapping_keycodes(modmapReply);
		for (ubyte i = 0; i != 8; i++)
			for (ubyte j = 0; j != modmapReply.keycodes_per_modifier; j++) {
				const xcb_keycode_t kcode = modmap[i * modmapReply.keycodes_per_modifier + j];

				foreach (key, val; mods) {
					if (!val || key in modifiers)
						continue;

					for (xcb_keycode_t* cur = val; *cur; cur++) {
						if (*cur != kcode)
							continue;
						modifiers[key] = cast(Modifier)(1 << i);
						break;
					}
				}
			}

		foreach (key, val; mods)
			xcb_free(val);
		xcb_free(modmapReply);

		void combine(string output, string in1, string in2) {
			Modifier in1d = cast(Modifier)0;
			Modifier in2d = cast(Modifier)0;
			if (auto i1 = in1 in modifiers)
				in1d = *i1;
			if (auto i2 = in2 in modifiers)
				in2d = *i2;

			modifiers[output] = in1d | in2d;
		}

		combine("alt", "alt_l", "alt_r");
		combine("shift", "shift_l", "shift_r");
		combine("control", "control_l", "control_r");

		combine("ctrl_l", "control_l", "");
		combine("ctrl_r", "control_r", "");
		combine("ctrl", "ctrl_l", "ctrl_r");

		combine("super", "super_l", "super_r");
		combine("hyper", "hyper_l", "hyper_r");

		modifiers["mod1"] = Modifier.Mod1;
		modifiers["mod2"] = Modifier.Mod2;
		modifiers["mod3"] = Modifier.Mod3;
		modifiers["mod4"] = Modifier.Mod4;
		modifiers["mod5"] = Modifier.Mod5;
		modifiers["lock"] = Modifier.Lock;
	}
}

