var counter = 0;

Info.AddCtor("quit.ds", function() {
	Keyboard.Bind("Ctrl + Q", function(var pressed) {
		Engine.Quit();
	});

	Keyboard.Bind("Ctrl + W", function(var pressed) {
		if (pressed == false)
			return;
		counter += 1;
		if (counter > 2)
			counter = 0;

		Mouse.Style(counter);
	});

});